{
  "name": "trading-helper",
  "version": "3.0.0",
  "description": "",
  "scripts": {
    "glogin": "clasp login",
    "glogout": "clasp logout",
    "gcreate": "clasp create --title trading-helper-dev --type standalone --rootDir ./apps-script",
    "gedit": "clasp open -P ./apps-script",
    "gopen": "clasp open -P ./apps-script --webapp",
    "gpush": "npm run build && clasp push -P ./apps-script -f",
    "gstart": "clasp push -P ./apps-script -w -f",
    "gbuild": "node gas-build.js",
    "start": "parcel src/web/index.html --dist-dir ./apps-script",
    "report": "parcel build src/web/index.tsx --dist-dir ./parcel-bundle-reports --reporter @parcel/reporter-bundle-analyzer",
    "build": "npm run gbuild && parcel build src/web/index.html --dist-dir ./apps-script --no-cache",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\" --config ./.prettierrc",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@google/clasp": "2.4.1",
    "@parcel/reporter-bundle-analyzer": "2.6.2",
    "@types/google-apps-script": "1.0.49",
    "@types/google.script.client-side": "0.1.1",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "esbuild": "0.14.48",
    "esbuild-gas-plugin": "0.4.0",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.1.2",
    "eslint-plugin-googleappsscript": "1.0.4",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.1",
    "parcel": "2.6.2",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "author": "Bohdan Kovalov",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "11.9.3",
    "@emotion/styled": "11.9.3",
    "@mui/icons-material": "5.8.4",
    "@mui/material": "5.8.6",
    "lightweight-charts": "3.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-window": "1.8.7",
    "browser-or-node": "2.0.0"
  }
}
